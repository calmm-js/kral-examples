{
  "name": "kefir.react.atom-examples",
  "version": "1.0.0",
  "description": "Kefir+React+Atom Examples",
  "main": "public/app.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:css": "lessc src/*.less > public/bundle.css",
    "build:js": "export NODE_ENV=production && browserify src/app.js -t babelify -t envify -g uglifyify > public/app.js",
    "lint": "eslint src test",
    "prepublish": "npm run lint && npm run build",
    "watch": "npm run watch:js & npm run watch:css",
    "watch:css": "npm run build:css && watch-run -p 'src/*.less' 'npm run build:css'",
    "watch:js": "watchify src/app.js -v -t [ babelify --sourceMaps=inline ] -o public/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calmm-js/kefir.react.atom-examples.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/calmm-js/kefir.react.atom-examples/issues"
  },
  "homepage": "https://github.com/calmm-js/kefir.react.atom-examples#readme",
  "dependencies": {
    "kefir.atom": "^1.1.4",
    "kefir.react.html": "^1.1.2",
    "kefir": "^3.2.1",
    "partial.lenses": "^1.1.1",
    "ramda": "^0.19.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.6.5",
    "babel-plugin-transform-react-constant-elements": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "eslint": "2.2.x",
    "eslint-plugin-react": "^4.2.0",
    "less": "^2.6.1",
    "uglifyify": "^3.0.1",
    "watch-run": "^1.2.4",
    "watchify": "^3.7.0"
  }
}
